# 数据库配置
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_DATABASE=uiineed_todo
MYSQL_USER=uiineed_todo_user
MYSQL_PASSWORD=your_secure_db_password_here

# JWT配置
JWT_SECRET=your_jwt_secret_key_at_least_64_characters_long_random_string_here

# 微信开放平台配置
WECHAT_OPEN_APP_ID=your_wechat_app_id_here
WECHAT_OPEN_APP_SECRET=your_wechat_app_secret_here

# CORS配置（多个域名用逗号分隔）
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com,https://www.yourdomain.com

# 应用配置
SPRING_PROFILES_ACTIVE=prod
SERVER_PORT=8080

# 数据库连接池配置
DB_POOL_MAX_SIZE=20
DB_POOL_MIN_IDLE=5
DB_POOL_IDLE_TIMEOUT=300000
DB_POOL_CONNECTION_TIMEOUT=20000

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE_MAX_SIZE=10MB
LOG_FILE_MAX_HISTORY=30